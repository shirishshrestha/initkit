{
  "name": "initkit",
  "version": "1.0.0",
  "type": "module",
  "description": "A powerful CLI tool for scaffolding web projects with best practices",
  "main": "src/index.js",
  "bin": {
    "initkit": "./bin/index.js"
  },
  "scripts": {
    "start": "node bin/index.js",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "lint": "eslint src/**/*.js",
    "format": "prettier --write \"src/**/*.js\"",
    "prepublishOnly": "npm test && npm run lint"
  },
  "keywords": [
    "cli",
    "scaffold",
    "generator",
    "project-initializer",
    "boilerplate",
    "template",
    "starter",
    "web-development"
  ],
  "author": "Your Name <your.email@example.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/initkit.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/initkit/issues"
  },
  "homepage": "https://github.com/yourusername/initkit#readme",
  "engines": {
    "node": ">=14.0.0"
  },
  "files": [
    "bin/",
    "src/",
    "templates/",
    "README.md",
    "LICENSE"
  ],
  "dependencies": {
    "chalk": "^5.3.0",
    "commander": "^12.0.0",
    "inquirer": "^9.2.15",
    "ora": "^8.0.1",
    "fs-extra": "^11.2.0",
    "ejs": "^3.1.9",
    "validate-npm-package-name": "^5.0.0"
  },
  "devDependencies": {
    "eslint": "^8.56.0",
    "prettier": "^3.2.4",
    "jest": "^29.7.0",
    "@types/node": "^20.11.5"
  }
}
